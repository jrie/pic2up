{
  "developer": {
    "name": "JanRie",
    "url": "https://github.com/jrie"
  },
  "default_locale": "en",
  "browser_action": {
    "browser_style": true,
    "default_title": "pic2up - a PicFlash.org uploader",
    "default_popup": "pic2up_start.html",
    "default_icon": {
    "16": "icons/pic2up_16.png",
    "32": "icons/pic2up_32.png",
    "48": "icons/pic2up_48.png",
    "64": "icons/pic2up_64.png",
    "96": "icons/pic2up_96.png",
    "128": "icons/pic2up_128.png"
    }
  },
  "description": "A inofficial PicFlash.org image and video uploader browser addon",
  "icons": {
    "16": "icons/pic2up_16.png",
    "32": "icons/pic2up_32.png",
    "48": "icons/pic2up_48.png",
    "64": "icons/pic2up_64.png",
    "96": "icons/pic2up_96.png",
    "128": "icons/pic2up_128.png"
  },
  "homepage_url": "https://github.com/jrie/pic2up",
  "manifest_version": 2,
  "name": "pic2up",
  "version": "0.4.11",
  "permissions": ["https://*.picflash.org/tool.php", "<all_urls>", "storage", "unlimitedStorage"],
  "background": {
    "page": "pic2up.html"
  }
}
